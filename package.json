{
	"name": "shell",
	"version": "1.0.0",
	"description": "w0s.jp - shell - node",
	"license": "MIT",
	"author": "Saeki Tominaga",
	"type": "module",
	"scripts": {
		"build": "tsc",
		"build:watch": "tsc -w",
		"test": "jest --coverage",
		"config:tsconvert": "json2ts -i configure/schema -o configure/type",
		"config:validate:common": "ajv --spec=draft7 -s configure/schema/common.json -d configure/common.json",
		"config:validate:test": "ajv --spec=draft7 -s configure/schema/test.json -d configure/test.json",
		"config:validate:amazon-ads": "ajv --spec=draft7 -s configure/schema/amazon-ads.json -d configure/amazon-ads.json",
		"config:validate:crawler-news": "ajv --spec=draft7 -s configure/schema/crawler-news.json -d configure/crawler-news.json",
		"config:validate:crawler-resource": "ajv --spec=draft7 -s configure/schema/crawler-resource.json -d configure/crawler-resource.json",
		"config:validate:twitter-user-info-history-kumeta": "ajv --spec=draft7 -s configure/schema/twitter-user-info-history-kumeta.json -d configure/twitter-user-info-history-kumeta.json",
		"config:validate:twitter-user-info-history-madoka": "ajv --spec=draft7 -s configure/schema/twitter-user-info-history-madoka.json -d configure/twitter-user-info-history-madoka.json",
		"config:validate:yokohama-library-hold-notice": "ajv --spec=draft7 -s configure/schema/yokohama-library-hold-notice.json -d configure/yokohama-library-hold-notice.json",
		"shell:Test": "node dist/Shell.js Test 0",
		"shell:AmazonDp": "node dist/Shell.js AmazonDp 10",
		"shell:CrawlerNews": "node dist/Shell.js CrawlerNews 10",
		"shell:CrawlerResource": "node dist/Shell.js CrawlerResource 10",
		"shell:TwitterUserInfoHistoryKumeta": "node dist/Shell.js TwitterUserInfoHistoryKumeta 0 true",
		"shell:TwitterUserInfoHistoryMadoka": "node dist/Shell.js TwitterUserInfoHistoryMadoka 0",
		"shell:YokohamaLibraryHoldNotice": "node dist/Shell.js YokohamaLibraryHoldNotice 10"
	},
	"dependencies": {
		"@saekitominaga/mime-parser": "^1.1.0",
		"abort-controller": "^3.0.0",
		"amazon-paapi": "^1.0.3",
		"header-case": "^2.0.4",
		"jsdom": "^16.4.0",
		"log4js": "^6.3.0",
		"node-fetch": "^2.6.1",
		"nodemailer": "^6.5.0",
		"puppeteer-core": "^8.0.0",
		"relative-to-absolute-iri": "^1.0.6",
		"sqlite": "^4.0.19",
		"sqlite3": "^5.0.2",
		"twitter": "^1.7.1",
		"twitter-text": "^3.1.0",
		"uuid": "^8.3.2"
	},
	"devDependencies": {
		"@babel/core": "^7.13.1",
		"@babel/plugin-transform-modules-commonjs": "^7.13.0",
		"@babel/preset-env": "^7.13.5",
		"@log4js-node/smtp": "^1.1.0",
		"@types/jsdom": "^16.2.6",
		"@types/node": "^14.14.31",
		"@types/node-fetch": "^2.5.8",
		"@types/nodemailer": "^6.4.0",
		"@types/twitter": "^1.7.0",
		"@types/twitter-text": "^3.1.0",
		"@types/uuid": "^8.3.0",
		"@typescript-eslint/eslint-plugin": "^4.15.2",
		"@typescript-eslint/parser": "^4.15.2",
		"ajv-cli": "^4.1.0",
		"babel-jest": "^26.6.3",
		"eslint": "^7.20.0",
		"eslint-config-prettier": "^8.1.0",
		"eslint-plugin-jest": "^24.1.5",
		"eslint-plugin-jsdoc": "^32.2.0",
		"jest": "^26.6.3",
		"json-schema-to-typescript": "^10.1.3",
		"paapi5-typescript-sdk": "^0.2.0",
		"typescript": "^4.2.2"
	}
}
