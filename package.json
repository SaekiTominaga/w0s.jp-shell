{
	"name": "shell",
	"version": "1.0.0",
	"description": "w0s.jp - shell - node",
	"license": "MIT",
	"author": "Saeki Tominaga",
	"type": "module",
	"scripts": {
		"build": "tsc",
		"build:watch": "tsc -w",
		"test": "jest --coverage",
		"config:validate:Common": "ajv --spec=draft7 -s configure/schema/Common.json -d configure/Common.json",
		"config:validate:Test": "ajv --spec=draft7 -s configure/schema/Test.json -d configure/Test.json",
		"config:validate:AmazonDp": "ajv --spec=draft7 -s configure/schema/AmazonDp.json -d configure/AmazonDp.json",
		"config:validate:CrawlerNews": "ajv --spec=draft7 -s configure/schema/CrawlerNews.json -d configure/CrawlerNews.json",
		"config:validate:CrawlerResource": "ajv --spec=draft7 -s configure/schema/CrawlerResource.json -d configure/CrawlerResource.json",
		"config:validate:TwitterUserInfoHistoryKumeta": "ajv --spec=draft7 -s configure/schema/TwitterUserInfoHistoryKumeta.json -d configure/TwitterUserInfoHistoryKumeta.json",
		"config:validate:TwitterUserInfoHistoryMadoka": "ajv --spec=draft7 -s configure/schema/TwitterUserInfoHistoryMadoka.json -d configure/TwitterUserInfoHistoryMadoka.json",
		"config:tsconvert": "json2ts -i configure/schema -o configure/type",
		"shell:Test": "node dist/Shell.js Test 0",
		"shell:AmazonDp": "node dist/Shell.js AmazonDp 10",
		"shell:CrawlerNews": "node dist/Shell.js CrawlerNews 10",
		"shell:CrawlerResource": "node dist/Shell.js CrawlerResource 10",
		"shell:TwitterUserInfoHistoryKumeta": "node dist/Shell.js TwitterUserInfoHistoryKumeta 0 true",
		"shell:TwitterUserInfoHistoryMadoka": "node dist/Shell.js TwitterUserInfoHistoryMadoka 0"
	},
	"dependencies": {
		"@saekitominaga/mime-parser": "^1.0.1",
		"amazon-paapi": "^1.0.3",
		"jsdom": "^16.4.0",
		"log4js": "^6.3.0",
		"node-fetch": "^2.6.1",
		"nodemailer": "^6.4.18",
		"relative-to-absolute-iri": "^1.0.6",
		"sqlite": "^4.0.19",
		"sqlite3": "^5.0.2",
		"twitter": "^1.7.1",
		"twitter-text": "^3.1.0",
		"uuid": "^8.3.2"
	},
	"devDependencies": {
		"@babel/core": "^7.12.16",
		"@babel/plugin-transform-modules-commonjs": "^7.12.13",
		"@babel/preset-env": "^7.12.16",
		"@log4js-node/smtp": "^1.1.0",
		"@types/jsdom": "^16.2.6",
		"@types/node": "^14.14.28",
		"@types/node-fetch": "^2.5.8",
		"@types/nodemailer": "^6.4.0",
		"@types/twitter": "^1.7.0",
		"@types/twitter-text": "^3.1.0",
		"@types/uuid": "^8.3.0",
		"@typescript-eslint/eslint-plugin": "^4.15.1",
		"@typescript-eslint/parser": "^4.15.1",
		"ajv-cli": "^4.1.0",
		"babel-jest": "^26.6.3",
		"eslint": "^7.20.0",
		"eslint-config-prettier": "^7.2.0",
		"eslint-plugin-jest": "^24.1.3",
		"eslint-plugin-jsdoc": "^32.0.1",
		"jest": "^26.6.3",
		"json-schema-to-typescript": "^10.1.3",
		"paapi5-typescript-sdk": "^0.1.1",
		"typescript": "^4.1.5"
	}
}
