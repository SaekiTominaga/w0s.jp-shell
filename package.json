{
	"name": "w0s.jp-shell",
	"version": "1.0.0",
	"description": "shell",
	"license": "MIT",
	"author": "Saeki Tominaga",
	"type": "module",
	"scripts": {
		"build-watch": "tsc --project node/tsconfig.json -w",
		"build-all": "tsc --project node/tsconfig.json",
		"lint": "eslint node/src/**/*.ts node/__tests__/*.test.js node/@types/*.d.ts",
		"test": "jest --coverage",
		"config-convert": "json2ts -i node/configure/schema -o node/configure/type",
		"config-validate": "run-p _config-validate:*",
		"shell-Test": "node node/dist/Shell.js -c Test -t 0",
		"shell-AmazonAds": "node node/dist/Shell.js -c AmazonAds -t 10",
		"shell-CrawlerNews": "node node/dist/Shell.js -c CrawlerNews -t 10",
		"shell-CrawlerResource": "node node/dist/Shell.js -c CrawlerResource -t 10",
		"shell-JrCyberStation": "node node/dist/Shell.js -c JrCyberStation -t 20",
		"shell-ThumbImage": "node node/dist/Shell.js -c ThumbImage -t 30 --dev",
		"shell-TwitterUserInfoHistoryKumeta": "node node/dist/Shell.js -c TwitterUserInfoHistoryKumeta -t 0 --dev",
		"shell-YokohamaLibraryHoldNotice": "node node/dist/Shell.js -c YokohamaLibraryHoldNotice -t 10",
		"_config-validate:amazon-ads": "ajv --spec=draft7 -s node/configure/schema/amazon-ads.json -d node/configure/amazon-ads.json",
		"_config-validate:common": "ajv --spec=draft7 -s node/configure/schema/common.json -d node/configure/common.json",
		"_config-validate:crawler-news": "ajv --spec=draft7 -s node/configure/schema/crawler-news.json -d node/configure/crawler-news.json",
		"_config-validate:crawler-resource": "ajv --spec=draft7 -s node/configure/schema/crawler-resource.json -d node/configure/crawler-resource.json",
		"_config-validate:jr-cyber-station": "ajv --spec=draft7 -s node/configure/schema/jr-cyber-station.json -d node/configure/jr-cyber-station.json",
		"_config-validate:test": "ajv --spec=draft7 -s node/configure/schema/test.json -d node/configure/test.json",
		"_config-validate:thumb-image": "ajv --spec=draft7 -s node/configure/schema/thumb-image.json -d node/configure/thumb-image.json",
		"_config-validate:twitter-user-info-history-kumeta": "ajv --spec=draft7 -s node/configure/schema/twitter-user-info-history-kumeta.json -d node/configure/twitter-user-info-history-kumeta.json",
		"_config-validate:yokohama-library-hold-notice": "ajv --spec=draft7 -s node/configure/schema/yokohama-library-hold-notice.json -d node/configure/yokohama-library-hold-notice.json"
	},
	"dependencies": {
		"@saekitominaga/mime-parser": "^1.2.3",
		"abort-controller": "^3.0.0",
		"amazon-paapi": "^1.0.7",
		"dayjs": "^1.11.6",
		"file-type": "^18.0.0",
		"header-case": "^2.0.4",
		"jsdom": "^20.0.2",
		"log4js": "^6.7.0",
		"node-fetch": "^3.2.10",
		"nodemailer": "^6.8.0",
		"paapi5-typescript-sdk": "^0.2.0",
		"puppeteer-core": "^19.2.2",
		"relative-to-absolute-iri": "^1.0.7",
		"sqlite": "^4.1.2",
		"sqlite3": "^5.1.2",
		"twitter-api-v2": "^1.12.9",
		"twitter-text": "^3.1.0",
		"uuid": "^9.0.0"
	},
	"devDependencies": {
		"@babel/core": "^7.20.2",
		"@babel/plugin-transform-modules-commonjs": "^7.19.6",
		"@babel/preset-env": "^7.20.2",
		"@log4js-node/smtp": "^2.0.8",
		"@types/jsdom": "^20.0.1",
		"@types/node": "^18.11.9",
		"@types/node-fetch": "^3.0.2",
		"@types/nodemailer": "^6.4.6",
		"@types/twitter-text": "^3.1.4",
		"@types/uuid": "^8.3.4",
		"@w0s/eslint-config": "^1.2.0",
		"@w0s/tsconfig": "^1.0.1",
		"ajv-cli": "^5.0.0",
		"babel-jest": "^29.3.1",
		"jest": "^29.3.1",
		"json-schema-to-typescript": "^11.0.2",
		"npm-run-all": "^4.1.5",
		"typescript": "^4.8.4"
	}
}
